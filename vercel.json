{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ],
    "crons": [
        {
        "path": "/api/cron",
        "schedule": "* * * * *",
        "script": "python3 cron.py"
        }
    ]
}